  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/secure-template.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "bundle-dts": "^1.0.4",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "mocha": "^6.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
