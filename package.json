{
  "name": "secure-template",
  "version": "1.0.2",
  "description": "Format strings in a secure manner and also can access nested properties.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/*.ts",
    "ts": "ts-node",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "dts": "gulp dts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run lint&&webpack&&npm run dts"
  },
  "keywords": [
    "format",
    "template",
    "compile",
    "string",
    "typescript"
  ],
  "author": "fisher <i.yu@qq.com>",
